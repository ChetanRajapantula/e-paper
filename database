CREATE TABLE  "COMPLAINT" 
   (	"FULLNAME" VARCHAR2(30) NOT NULL ENABLE, 
	"EMAIL" VARCHAR2(30) NOT NULL ENABLE, 
	"DEPT" VARCHAR2(30) NOT NULL ENABLE, 
	"COMP" VARCHAR2(500) NOT NULL ENABLE
   )
/



CREATE TABLE  "PHOTOS" 
   (	"ID" NUMBER(5,0), 
	"TITLE" VARCHAR2(50), 
	"PHOTO" BLOB, 
	 PRIMARY KEY ("ID") ENABLE
   )
/


CREATE TABLE  "GLOGIN" 
   (	"GID" NUMBER NOT NULL ENABLE, 
	"NAME" VARCHAR2(20) NOT NULL ENABLE
   )
/
